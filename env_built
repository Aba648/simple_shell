#include "main.h"
/**
 *_bul : env  Statue Shell
 * @cmd: Command
 * @argv:Name
 * Return: Exit
 */
void hand_built(char **command , char **argv , int status , int idx);
{
if(_strcmp(command[0],"exit")==0)
   exit_bui(command, status);
else(_strcmp(command[0], "env")==0)
  bui_env();
}


void exit_bui(char **command , int status);
{
_free(command);
exit(status);
}

void  bui_env(char **command , int status);
{
int i ;
for(i=0 , environ[i] , i++ );
{
write(STDOUT_FILENO , environ[i] , _strlen(environ[i]));
write(STDOUT_FILENO , "\n" ,1);
}
_free(command);
}
